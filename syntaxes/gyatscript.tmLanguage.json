{
    "scopeName": "source.gyat",
    "name": "GyatScript",
    "patterns": [
      {
        "match": "\\b(ts|ong|icl|gng|spit|gyatlist)\\b",
        "name": "keyword.control.gyat"
      },
      {
        "match": "\\b(sup sigma|duh sigma)\\b",
        "name": "keyword.operator.sigma.gyat"
      },
      {
        "match": "\\b(true|false|null|undefined)\\b",
        "name": "constant.language.gyat"
      },
      {
        "match": "\\b\\d+\\b",
        "name": "constant.numeric.gyat"
      },
      {
        "match": "\".*?\"",
        "name": "string.quoted.double.gyat"
      },
      {
        "match": "'[^']*'",
        "name": "string.quoted.single.gyat"
      },
      {
        "match": "//.*",
        "name": "comment.line.double-slash.gyat"
      },
      {
        "begin": "/\\*",
        "end": "\\*/",
        "name": "comment.block.gyat"
      },
      {
        "match": "\\b([A-Z_][A-Z0-9_]*)\\b",
        "name": "constant.other.gyat"
      }
    ]
  }
  